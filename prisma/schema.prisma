// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Event {
    id        String   @id @default(cuid())
    type      String
    pageUrl   String?
    visitorId String
    metadata  Json?
    createdAt DateTime @default(now())
}